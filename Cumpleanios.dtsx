                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select TRIM([TIPO DOCUMENTO]) [TIPO DOCUMENTO], TRIM([documento identidad]) [documento identidad], TRIM(upper([1ER NOMBRE])) [1ER NOMBRE] , TRIM(upper([2DO NOMBRE])) [2DO NOMBRE] , 

TRIM(upper([1ER APELLIDO])) [1ER APELLIDO] , convert(varchar(10), [FECHA NACIMIENTO], 23) as [FECHA NACIMIENTO],

TRIM(lower(coalesce([CORREO ELECTRONICO INSTITUCION], ''))) [CORREO ELECTRONICO INSTITUCION],  

TRIM(upper(coalesce(min([desc centro costo (car)]), ''))) Cargo

from (

SELECT 'CI' AS [TIPO DOCUMENTO],FUNCIONA.FUNDOCUMNR AS [DOCUMENTO IDENTIDAD],

FUNCIONA.FUNNOM1 AS [1ER NOMBRE],

FUNCIONA.FUNNOM2 AS [2DO NOMBRE],FUNCIONA.FUNAPE1 AS [1ER APELLIDO],

FUNCIONA.FUNNACIM AS [FECHA NACIMIENTO],FUNATRVAF14.FUNAVALOR AS [CORREO ELECTRONICO INSTITUCION],

ccNodum.nom_dpto AS [DESC CENTRO COSTO (CAR)],

FUNCIONA.FUNID,FUNCIONA.FUNID AS [Nº FUNCIONARIO]

FROM (((FUNCIONA LEFT JOIN CARGOSFU ON FUNCIONA.FUNID=CARGOSFU.FUNID)

LEFT JOIN CARGOSCC ON CARGOSCC.FUNID=CARGOSFU.FUNID AND CARGOSCC.FUNAL=CARGOSFU.FUNAL)

LEFT JOIN CENTROCO CENTROCOF ON CENTROCOF.CCID=CARGOSCC.FUNCCID)

left join [ucudbs09].[nodum_prod].[dbo].[ct_dptos] ccNodum on CENTROCOF.CCID = ccNodum.cod_dpto collate Modern_Spanish_CI_AS

LEFT JOIN FUNATRVA FUNATRVAF14 ON (FUNATRVAF14.FUNID=FUNCIONA.FUNID AND FUNATRVAF14.FUNAL=0 AND FUNATRVAF14.FUNAID=14)

WHERE NOT(FUNFECBAJA &gt;= '01/12/00' AND FUNFECBAJA &lt;= EOMONTH (getdate() ))

AND NOT(FUNCARID = 37 OR FUNCARID = 169 OR FUNCARID = 682 OR FUNCARID = 683 OR FUNCARID = 681 OR

FUNCARID = 684 OR FUNCARID = 685 OR FUNCARID = 41 OR FUNCARID = 36 OR FUNCARID = 35 OR FUNCARID = 40 OR FUNCARID = 11 OR FUNCARID = 38 OR FUNCARID = 42 OR

FUNCARID = 34 OR FUNCARID = 680 OR FUNCARID = 39 OR FUNCARID = 12 OR FUNCARID = 13 OR FUNCARID = 10 OR FUNCARID = 16) AND NOT(FUNCIONA.FUNID = 0)

AND CARGOSFU.FunAL&lt;100

) GENINF01

group by [TIPO DOCUMENTO], [documento identidad], [1ER NOMBRE], [2DO NOMBRE], [1ER APELLIDO], [FECHA NACIMIENTO],[CORREO ELECTRONICO INSTITUCION], [FunId]

order by day([fecha nacimiento])</property>
